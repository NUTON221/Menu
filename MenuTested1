local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "NeytronHub",
   Icon = 0,
   LoadingTitle = "–ó–∞–≥—Ä—É–∑–∫–∞...",
   LoadingSubtitle = "by You",
   ShowText = "NeytronHub",
   Theme = "Amethyst", -- üåå —Ç–µ–º–∞
   ToggleUIKeybind = "K",

   ConfigurationSaving = {
      Enabled = true,
      FolderName = "NeytronHub",
      FileName = "Settings"
   },

   Discord = {
      Enabled = false
   },

   KeySystem = false
})

---------------------------------------------------------
-- –¢–≤–æ–π —Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª
---------------------------------------------------------

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Remotes = ReplicatedStorage:WaitForChild("Remotes")
local StartLocalPlayerTeleport = Remotes:WaitForChild("StartLocalPlayerTeleport")

local loopEnabled = false
local delayTime = 15

local function safeFire(mapId)
    local args = {
        { mapId = mapId }
    }
    local ok, err = pcall(function()
        StartLocalPlayerTeleport:FireServer(unpack(args))
    end)
    if not ok then
        warn("–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ—Ç–ø—Ä–∞–≤–∫–µ StartLocalPlayerTeleport:", err)
    end
end

-- –°–æ–∑–¥–∞—ë–º –≤–∫–ª–∞–¥–∫—É –∏ —Å–µ–∫—Ü–∏—é
local MainTab = Window:CreateTab("Main", 4483362458)
local MainSection = MainTab:CreateSection("Mains")

-- –°–ª–∞–π–¥–µ—Ä –∑–∞–¥–µ—Ä–∂–∫–∏
MainTab:CreateSlider({
    Name = "–í—Ä–µ–º—è –º–µ–∂–¥—É —Ç–ø",
    Range = {1, 60},
    Increment = 1,
    Suffix = "—Å–µ–∫",
    CurrentValue = 15,
    Flag = "DelayTime",
    Callback = function(value)
        delayTime = value
        print("–ù–æ–≤–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞:", delayTime, "—Å–µ–∫—É–Ω–¥")
    end,
})

-- –¢—É–º–±–ª–µ—Ä –∑–∞–ø—É—Å–∫–∞ —Ü–∏–∫–ª–∞
MainTab:CreateToggle({
    Name = "–¶–∏–∫–ª —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞ 50001-50009",
    CurrentValue = false,
    Flag = "TeleportLoop",
    Callback = function(state)
        loopEnabled = state

        if loopEnabled then
            print("–¶–∏–∫–ª —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞ –∑–∞–ø—É—â—ë–Ω")
            task.spawn(function()
                while loopEnabled do
                    for id = 50001, 50009 do
                        if not loopEnabled then break end

                        safeFire(id)
                        print("–û—Ç–ø—Ä–∞–≤–ª–µ–Ω mapId:", id)

                        local step = 0.25
                        local elapsed = 0
                        while elapsed < delayTime and loopEnabled do
                            task.wait(step)
                            elapsed += step
                        end
                        if not loopEnabled then break end
                    end
                end
                print("–¶–∏–∫–ª —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞ –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
            end)
        else
            print("–¶–∏–∫–ª —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞ –≤—ã–∫–ª—é—á–µ–Ω")
        end
    end
})

	or input.UserInputType == Enum.UserInputType.Touch then
		update(input)
	end
end)
